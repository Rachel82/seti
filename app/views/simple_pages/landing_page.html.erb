<div class="line-separator"></div>
<h4 class="featured_post">latest in</h4>


<div class="container">
  <div class="row">
    <% @posts.each do |post| %>
    <div class=" col-md-6">
      <a href="#"><p><%= post.category.name %></p></a>
      <h3><%= post.title %></h3>
      <p>January 1, 2018<p>
      <%= image_tag("test.jpg", class: "img-responsive") %>
      <p><%= post.content %> <%= link_to 'Show', post %></p>
    </div><!-- /col -->
    <% end %>
  </div>
</div>

